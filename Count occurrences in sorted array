# include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cout<<"Enter the length of the array "<<endl;
    cin>>n; 
    int x;
    cout<<"Enter the number of which you have to count the occurences "<<endl;
    cin>>x;
    int arr[n];
    int count =0;
    cout<<"Enter your array  "<<endl;
    for(int i =0; i<n; i++){
        cin>>arr[i];
    }
    
    sort(arr, arr+n);
    for(int i =0; i<n; i++){
        if(arr[i]==x){
            count++;
        }
    }
        cout<<"Total number of occurence of number x "<<count;
    
}
